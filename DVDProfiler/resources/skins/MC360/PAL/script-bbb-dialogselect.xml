<window type="dialog" id="2000">
	<defaultcontrol always="true">3</defaultcontrol>
	<animation effect="fade" time="200">WindowOpen</animation>
	<animation effect="fade" time="200">WindowClose</animation>
	<controls>
		<include>TheDialogs</include>
		<control type="group">
			<animation effect="fade" delay="450" time="200">WindowOpen</animation>
			<animation effect="fade" time="200">WindowClose</animation>
			<posx>0</posx>
			<posy>0</posy>
			<width>350</width>
			<height>450</height>
			<control type="label" id="1">
				<description>heading label</description>
				<posx>365</posx>
				<posy>40</posy>
				<width>350</width>
				<align>left</align>
				<label></label>
				<include>DialogTitle</include>
			</control>
			<control type="label" id="6">
				<description>column 1 heading label</description>
				<posx>365</posx>
				<posy>60</posy>
				<width>250</width>
				<height>15</height>
				<align>left</align>
				<aligny>center</aligny>
				<font>font11</font>
				<label>LABEL 1</label>
				<textcolor>TextcolorBlack</textcolor>
			</control>
			<control type="label" id="7">
				<description>column 2 heading label</description>
				<posx>10r</posx>
				<posy>60</posy>
				<width>250</width>
				<height>15</height>
				<align>right</align>
				<aligny>center</aligny>
				<font>font12</font>
				<label>LABEL 2</label>
				<textcolor>TextcolorBlack</textcolor>
			</control>

			<control type="list" id="3">
				<posx>365</posx>
				<posy>85</posy>
				<width>350</width>
				<height>360</height>
				<onup>3</onup>
				<ondown>3</ondown>
				<onleft>61</onleft>
				<onright>61</onright>
				<pagecontrol>61</pagecontrol>
				<scrolltime>200</scrolltime>
				<itemlayout height="30">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>350</width>
						<height>28</height>
						<texture border="3">iconlist-nofocus.png</texture>
					</control>
					<control type="label">
						<posx>5</posx>
						<posy>0</posy>
						<width>350</width>
						<height>28</height>
						<font>font12</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>TextcolorBlack</textcolor>
						<selectedcolor>listselectedcolor</selectedcolor>
						<info>ListItem.Label</info> 
					</control>
					<control type="label">
						<posx>345</posx>
						<posy>0</posy>
						<width>200</width>
						<height>28</height>
						<font>font121</font>
						<align>right</align>
						<aligny>center</aligny>
						<textcolor>TextcolorBlack</textcolor>
						<selectedcolor>listselectedcolor</selectedcolor>
						<info>ListItem.Label2</info>
					</control> 
				</itemlayout>
				<focusedlayout height="30">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>350</width>
						<height>28</height>
						<visible>!Control.HasFocus(3)</visible>
						<texture border="3">iconlist-nofocus.png</texture>
					</control>
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>350</width>
						<height>28</height>
						<visible>Control.HasFocus(3)</visible>
						<texture border="3">iconlist-focus.png</texture>
					</control>
					<control type="label">
						<posx>5</posx>
						<posy>0</posy>
						<width>350</width>
						<height>28</height>
						<font>font12</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>TextcolorBlack</textcolor>
						<selectedcolor>listselectedcolor</selectedcolor>
						<info>ListItem.Label</info>
					</control>
					<control type="label">
						<posx>345</posx>
						<posy>0</posy>
						<width>200</width>
						<height>28</height>
						<font>font11</font>
						<aligny>center</aligny>
						<textcolor>TextcolorBlack</textcolor>
						<selectedcolor>listselectedcolor</selectedcolor>
						<align>right</align>
						<info>ListItem.Label2</info>
					</control> 
				</focusedlayout>
			</control>
			<control type="spincontrol" id="61">
				<description>Next page button</description>
				<posx>680</posx>
				<posy>450</posy>
				<subtype>page</subtype>
				<font>font12</font>
				<onleft>3</onleft>
				<onright>3</onright>
				<ondown>3</ondown>
				<onup>3</onup>
				<textcolor>TextcolorBlack</textcolor>
				<showonepage>false</showonepage>
			</control>
		</control>
	</controls>
</window>